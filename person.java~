public class person{
    private boolean canHandle;
    private int money;
    public person(){
	Random r = new Random();
	boolean[] i = {true,false}
	canHandle = i[r.nextInt(2)];
	money = (r.nextInt(10) + 1) * 10; 
    }
    
    public person(boolean canHandle, int money){
	this.canHandle = canHandle;
	this.money = money;
    }

    public boolean puke{
    }
}